{
    "extends": ["vgno", "prettier"],
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "trailingComma": "es5",
            "tabWidth": 4,
            "printWidth": 120
        }]
    }
}